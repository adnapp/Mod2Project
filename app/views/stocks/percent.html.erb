<marquee class="back double">
<% @percent.each do |stock| %>
<%= stock.company_name %> - 
<% end %>
</marquee>
<br>
<br>
<div>
<h1>Largest Percentage</h1>
<br>
<% @percent.each do |stock|%>
<p><%= link_to stock.company_name, stock_show_path(stock.symbol) %></p>
<%end%>
</div>
<br>
<div>
<%= form_tag stock_show_path(:search), method: :get do %>
  <h2><%= label_tag "Search for a stock by ticker"%></h2>
  <br>
  <%= search_field_tag :search %>
    <%= submit_tag("Search")  %>
 <%end%>
</div>
<br>
<%= link_to "Main Page", stocks_path %>
<br>
<br>
